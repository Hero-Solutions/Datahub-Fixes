                    )

                    lido_basenameset(
                        descriptiveMetadata.objectRelationWrap.subjectWrap.subjectSet.$last.subject.subjectEvent.$last.event.eventName,
                        'c.content\.subject'
                    )

                    if all_match('c.content\.subject\.lref', '.*\S.*')

