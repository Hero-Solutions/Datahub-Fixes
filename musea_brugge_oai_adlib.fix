                    else

                        if all_match('c.content\.subject\.lref', '.*\S.*')

                            lido_term(
                                descriptiveMetadata.objectRelationWrap.subjectWrap.subjectSet.$append.subject.subjectConcept.$append,
                                'c.content\.subject',
                                -conceptid: 'c.content\.subject\.lref',
                                -type: local,
                                -source: Adlib
                            )

                        else

                            lido_term(
                                descriptiveMetadata.objectRelationWrap.subjectWrap.subjectSet.$append.subject.subjectConcept.$append,
                                'c.content\.subject'
                            )

