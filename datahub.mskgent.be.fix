                            replace_all(or_record.french_term, '^(\d(?:\d(?:([A-Z])(?:\2)?\d*)?)?\()[^\+][^\)]*(\)\d*(?:\(\+\d+\))?.*)', '$1...$3')

                            lookup_in_store(or_record.french_term, DBI, data_source: "dbi:SQLite:/tmp/import.iconclass_fr.sqlite")

                            if all_match(or_record.french_term.t, '.*\(avec [^\(]*[A-Z][A-Z]+[^\(]*\).*')

                                replace_all('or_record.french_term_tmp', '^\d(?:\d(?:([A-Z])(?:\1)?\d*)?)?\(([^\+][^\)]*)\)\d*(?:\(\+\d+\))?.*', '$2')
                                paste(or_record.french_term.t, 'or_record.french_term_tmp', or_record.french_term.t, join_char:'##KEYWORD##')
                                replace_all(or_record.french_term.t, '(.*)##KEYWORD##(.*)\(avec [^\(]*[A-Z][A-Z]+[^\(]*\)(.*)', '$1##KEYWORD##$2\($1\)$3')
                                replace_all(or_record.french_term.t, '(.*)##KEYWORD##(.*)\(avec [^\(]*[A-Z][A-Z]+[^\(]*\)(.*)', '$1##KEYWORD##$2\($1\)$3')
                                replace_all(or_record.french_term.t, '.*##KEYWORD##(.*)', '$1')

                            end

                        end

                    end

                    if all_match(or_record.french_term.t, '.*\S.*')

