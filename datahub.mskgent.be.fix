                        paste(or_record.bibliography_display, or_record.bibliography_display, p.publisher, join_char:' : ')

                    else

                        paste(or_record.bibliography_display, or_record.bibliography_display, p.publisher, join_char:' ; ')

                    end

                end

                if all_match(p.year_of_publication, '.*\S.*')

                    if all_match(p.publisher, '.*\S.*')

                        paste(or_record.bibliography_display, or_record.bibliography_display, p.year_of_publication, join_char:', ')

                    else

                        if all_match(p.place_of_publication, '.*\S.*')

                            paste(or_record.bibliography_display, or_record.bibliography_display, p.year_of_publication, join_char:' : ')

                        else

                            paste(or_record.bibliography_display, or_record.bibliography_display, p.year_of_publication, join_char:' ; ')

                        end

                    end

                end

            end

        end

        remove_field(or_record.count)

        if all_match('c.documentation\.page_reference', '.*\S.*')

            prepend('c.documentation\.page_reference', '(')
            append('c.documentation\.page_reference', ')')

            if exists(or_record.bibliography_display)

                paste(or_record.bibliography_display, or_record.bibliography_display, 'c.documentation\.page_reference', join_char:' ')

            else

                copy_field('c.documentation\.page_reference', or_record.bibliography_display)

            end

        end

        if all_match(or_record.bibliography_display, '.*\S.*')

            set_field(or_record.relationshipEn, 'Is Referenced By')
            set_field(or_record.relationshipURI, 'http://purl.org/dc/terms/isReferencedBy')

            lido_term(
                descriptiveMetadata.objectRelationWrap.relatedWorksWrap.relatedWorkSet.$last.relatedWorkRelType,
                or_record.relationshipEn,
                -conceptid: or_record.relationshipURI,
                -type: URI,
                -pref: preferred,
                -lang: en
            )

            copy_field(or_record.bibliography_display, descriptiveMetadata.objectRelationWrap.relatedWorksWrap.relatedWorkSet.$last.displayRelatedWork.$append._)
            add_field(descriptiveMetadata.objectRelationWrap.relatedWorksWrap.relatedWorkSet.$last.displayRelatedWork.$last.lang, 'nl')

        end

        remove_field(or_record.bibliography_display)
        remove_field(or_record.relationshipEn)
        remove_field(or_record.relationshipURI)

    end



