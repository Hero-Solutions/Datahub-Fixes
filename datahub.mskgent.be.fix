                            lookup_in_store(or_record.english_term, DBI, data_source: "dbi:SQLite:/tmp/import.iconclass_en.sqlite")

                            if all_match(or_record.english_term.t, '.*\(with [^\(]*[A-Z][A-Z]+[^\(]*\).*')

                                replace_all('or_record.english_term_tmp', '^\d(?:\d(?:([A-Z])(?:\1)?\d*)?)?\(([^\+][^\)]*)\)\d*(?:\(\+\d+\))?.*', '$2')
                                paste(or_record.english_term.t, 'or_record.english_term_tmp', or_record.english_term.t, join_char:'##KEYWORD##')
                                replace_all(or_record.english_term.t, '(.*)##KEYWORD##(.*)\(with [^\(]*[A-Z][A-Z]+[^\(]*\)(.*)', '$1##KEYWORD##$2\($1\)$3')
                                replace_all(or_record.english_term.t, '(.*)##KEYWORD##(.*)\(with [^\(]*[A-Z][A-Z]+[^\(]*\)(.*)', '$1##KEYWORD##$2\($1\)$3')
                                replace_all(or_record.english_term.t, '.*##KEYWORD##(.*)', '$1')

                            end

                        end

                    end

                    if all_match(or_record.english_term.t, '.*\S.*')

